{
	"containers": [
		{
			"prettyName": "PHP 7.0 RC 6 FPM WordPress 3.7.1 with Elasticsearch 2.0",
			"image": "dockunit/prebuilt-images:php-7.0-rc-6-fpm-mysql-phpunit-elasticsearch-2.0",
			"beforeScripts": [
				"service mysql start",
				"service elasticsearch start",
				"bash bin/install-wp-tests.sh wordpress_test root '' localhost 3.7.1"
			],
			"testCommand": "phpunit --group users"
		},
		{
			"prettyName": "PHP 7.0 RC 6 FPM WordPress Latest with Elasticsearch 2.0",
			"image": "dockunit/prebuilt-images:php-7.0-rc-6-fpm-mysql-phpunit-elasticsearch-2.0",
			"beforeScripts": [
				"service mysql start",
				"service elasticsearch start",
				"bash bin/install-wp-tests.sh wordpress_test root '' localhost latest"
			],
			"testCommand": "phpunit --group users"
		}
	]
}
